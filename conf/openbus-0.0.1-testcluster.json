{
  "environment_path": "../deploop-environments.git",
  "environment_branch": "buildoop-openbus-0.0.1",
  "environment_cluster": "testcluster",
  "buildoop_repository": "http://openbus-buildoop/buildoop/",
  "cluster_layout": 
  {
    "name": "redoop",
    "batch": 
    {
      "nn1": {
        "hostname": "redoop-nn1.openbus.org",
        "entity": ["flume", "monit"]
      },
      "nn2": {
        "hostname": "redoop-nn2.openbus.org",
        "entity": ["flume", "monit"]
      },
      "rm": {
        "hostname": "redoop-rm.openbus.org",
        "entity": ["flume", "monit"]
      },
      "dn": [
          { 
            "hostname":"redoop-dn1.openbus.org",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"redoop-dn2.openbus.org",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"redoop-dn3.openbus.org",  
            "entity": ["flume"]  
          }
      ]
    },
    "bus": 
    {
      "name": "kafka",
      "kafka-worker": [
          { 
            "hostname":"bus-01",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"bus-02",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"bus-03",  
            "entity": ["flume"]  
          }
      ]
    },
    "speed": 
    {
      "name": "storm",
      "storm-master": {
         "hostname": "speed-01",  
         "entity": ["flume"]  
       },
      "storm-worker": [
          { 
            "hostname":"speed-02",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"speed-03",  
            "entity": ["flume"]  
          }
      ]
    },
    "serving": 
    {
      "name": "cassandra",
      "cassandra-master": {
        "hostname": "serving-01",  
        "entity": ["flume"] 
      },
      "cassandra-worker": [
          { 
            "hostname":"serving-02",  
            "entity": ["flume"]  
          },
          { 
            "hostname":"serving-03",  
            "entity": ["flume"]  
          }
      ]
    }
  }
}
